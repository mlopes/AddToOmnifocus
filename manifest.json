{
  "manifest_version": 2,
  "name": "SendToOmniFocus",
  "version": "1.0",
  "description": "Send to OmniFocus",
  "homepage_url": "https://github.com/mlopes/AddToOmnifocus",
  "icons": {
    "16": "data/icon-16.png",
    "32": "data/icon-32.png",
    "64": "data/icon-64.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "data/icon-32.png",
    "default_title": "SendToOmniFocus"
  }
}